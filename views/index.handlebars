<div class="container">
  <div class="row justify-content-md-center">
    <div id="toBeEaten" class="col col-md-5">
      <h4>Your Order</h4>
      <br>
      <ul style="list-style: none;">
        {{#each burgers}}
        {{#unless this.consumed}}
        <li>
          <p class="label"> {{this.burger_name}}
          <button type="submit" id="{{this.id}}" class="eatBurger">Devour it!</button>
          <button type="submit" id="{{this.id}}" class="delBurger">X</button></p>
        </li>
        {{/unless}}
        {{/each}}
      </ul>
    </div>
    <div class="col-md-1">

    </div>
    <div id="eaten" class="col col-md-5">
      <h4>In Your Stomach</h4>
      <br>
      <ul style="list-style: none;">
        {{#each burgers}}
        {{#if this.consumed}}
        <li>
          <p id="stomachContent"class="label"> {{this.burger_name}}</p>
        </li>
        {{/if}}
        {{/each}}
      </ul>
    </div>
  </div>